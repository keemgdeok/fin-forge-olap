# Airflow core paths
AIRFLOW_HOME=.airflow
# Optional: override default Cosmos/dbt target
DBT_TARGET=dev
DBT_THREADS=4

# ClickHouse connection details
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=9440
CLICKHOUSE_HTTP_PORT=8443
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=
CLICKHOUSE_DATABASE=analytics
CLICKHOUSE_SCHEMA=analytics
CLICKHOUSE_SECURE=true
CLICKHOUSE_SECURE_HTTP=true
CLICKHOUSE_VERIFY=false
CLICKHOUSE_TIMEOUT=30
CLICKHOUSE_SECURE_SOCKET_TIMEOUT=30
CLICKHOUSE_CLUSTER=
CLICKHOUSE_COMPRESSION=lz4
CLICKHOUSE_ASYNC=false
CLICKHOUSE_DSN=clickhouse://default:@localhost:9440/analytics

# Optional service endpoints
FASTAPI_HOST=127.0.0.1
FASTAPI_PORT=8000
